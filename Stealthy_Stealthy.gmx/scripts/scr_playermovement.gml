if keyboard_check_direct(vk_right) || keyboard_check_direct(ord("D")){ x ++; }
if keyboard_check_direct(vk_left) || keyboard_check_direct(ord("A")){ x --; }
if keyboard_check_direct(vk_up) || keyboard_check_direct(ord("W")){ y --; }
if keyboard_check_direct(vk_down) || keyboard_check_direct(ord("S")){ y ++; }
